{
  "expo": {
    "name": "controll-emprestimo",
    "slug": "controll-emprestimo",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./utils/assets/images/icon.png",
    "scheme": "controllemprestimonative",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      // "adaptiveIcon": {
      //   "backgroundColor": "#E6F4FE"
      // },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "package": "br.com.controll"
    },
    "web": {
      "output": "static",
      "favicon": "./utils/assets/images/icon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./utils/assets/images/icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#FFFFFF"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "a5e8a43c-0736-431b-bdf0-36807a994f13"
      }
    }
  }
}